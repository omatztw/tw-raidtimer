{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended", "tslint-config-prettier"],
  "jsRules": {},
  "rules": {
    "prettier": [
      true,
      {
        "singleQuote": true,
        "semi": true,
        "printWidth": 120
      }
    ],
    "no-console": [false],
    "interface-name": [true, "never-prefix"],
    "object-literal-sort-keys": false
  },
  "rulesDirectory": ["tslint-plugin-prettier"]
}
